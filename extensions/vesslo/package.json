{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vesslo",
  "title": "Vesslo",
  "description": "Search and manage apps from Vesslo",
  "icon": "extension-icon.png",
  "author": "hjm79",
  "license": "MIT",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "commands": [
    {
      "name": "search-apps",
      "title": "Search Apps",
      "subtitle": "Vesslo",
      "description": "Search apps by name, developer, tags, or memo",
      "mode": "view"
    },
    {
      "name": "updates",
      "title": "View Updates",
      "subtitle": "Vesslo",
      "description": "View apps with available updates",
      "mode": "view"
    },
    {
      "name": "bulk-homebrew-update",
      "title": "Bulk Homebrew Update",
      "subtitle": "Vesslo",
      "description": "Update all Homebrew cask apps at once",
      "mode": "view"
    },
    {
      "name": "browse-by-tag",
      "title": "Browse by Tag",
      "subtitle": "Vesslo",
      "description": "View apps grouped by tags",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.66.0"
  },
  "overrides": {
    "@types/react": "18.2.27",
    "@types/react-dom": "18.2.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.10.0",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}